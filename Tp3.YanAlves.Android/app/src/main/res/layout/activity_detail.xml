<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yanal.tp3yanalvesandroid.DetailActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp"
        android:weightSum="1">

        <TextView
            android:id="@+id/txtDetailNome"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:textAlignment="center"
            android:text="TextView"
            android:layout_weight="0.09" />

        <TextView
            android:id="@+id/txtDetailSenha"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:textAlignment="center"
            android:text="TextView"
            android:layout_weight="0.09" />

        <TextView
            android:id="@+id/txtDetailEmail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:gravity="center_vertical"
            android:textAlignment="center"
            android:layout_weight="0.08" />

        <TextView
            android:id="@+id/txtDetailTelefone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:gravity="center_vertical"
            android:textAlignment="center"
            android:layout_weight="0.07" />

        <TextView
            android:id="@+id/txtDetailCelular"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:gravity="center_vertical"
            android:textAlignment="center"
            android:layout_weight="0.07" />

        <TextView
            android:id="@+id/txtDetailCpf"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:gravity="center_vertical"
            android:textAlignment="center"
            android:layout_weight="0.08" />

        <TextView
            android:id="@+id/txtDetailCidade"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:gravity="center_vertical"
            android:textAlignment="center"
            android:layout_weight="0.10" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
